#include<stdio.h>
#include<conio.h>
void main()
{
   int n,i,p=0;
   scanf("%d",&n);
   int a[n];
   for(i=0;i<n;i++)
   {
     scanf("%d",&a[i]);  
   }
   for(i=0;i<n;i++){
       if(a[i]!=0)
       printf("%d",a[i]);
  }
   for(i=0;i<n;i++)
  {
      if(a[i]==0){
         p++;
   }
   }
   for(i=0;i<p;i++)
   {
       printf("0");
   }
   
}
